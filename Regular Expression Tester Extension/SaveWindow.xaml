<Window x:Class="AndreasAndersen.Regular_Expression_Tester_Extension.SaveWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Save Regular Expression" Height="114" Width="345" ResizeMode="NoResize" Icon="/Regular%20Expression%20Tester%20Extension;component/Resources/SmallRegExIcon.ico" Loaded="Window_Loaded" WindowStartupLocation="CenterScreen">
    <Grid Margin="5">
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="4*" />
        </Grid.ColumnDefinitions>
        <TextBlock Grid.Row="0" Grid.Column="0">Name:</TextBlock>
        <TextBox Name="textBoxName" Grid.Row="0" Grid.Column="1" Width="240" Height="23" VerticalAlignment="Top" />
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Grid.Row="1" Grid.Column="1">
            <Button Content="Cancel" Height="23" Name="buttonCancel" VerticalAlignment="Top" Width="75" Margin="0, 0, 5, 0" IsCancel="True" />
            <Button Content="Ok" Height="23" Name="buttonOk" VerticalAlignment="Top" Width="75" Click="buttonOk_Click" IsDefault="True" />
        </StackPanel>
    </Grid>
</Window>
